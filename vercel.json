{
  "git": {
    "deploymentEnabled": {
      "master": false,
      "frontend": true
    }
  },
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://gamefast-backend.onrender.com/api/:path*"
    }
  ]
} 